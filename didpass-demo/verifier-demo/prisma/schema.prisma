// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model verification {
  id String @id
  dvr_id String
  dvr_title String
  time_stamp Int
  wallet String
  status Boolean
}

model dvr {
  dvr_id    String  @id
  dvr_title String
  query   String  @db.LongText
}

model api_keys {
  apiKey String @id
  email String
}